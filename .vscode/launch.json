{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Launch ML",
      "configurations": ["Launch ML Server", "Launch ML Client"]
    }
  ],
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch ML Client",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/ML/src/"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch ML Server",
      "program": "${workspaceFolder}/ML/build.js",
      "args": ["--dev"]
    }
  ]
}
